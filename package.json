{
  "name": "travo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.4",
    "@testing-library/user-event": "^12.0.11",
    "date-fns": "^2.14.0",
    "fontsource-roboto": "^2.1.4",
    "react": "^16.13.1",
    "react-datepicker": "^3.0.0",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.0.1",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.1",
    "uuid": "^8.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn build:app && yarn build:extension",
    "build:app": "INLINE_RUNTIME_CHUNK=false GENERATE_SOURCEMAP=false react-scripts build",
    "build:extension": "webpack --mode production --config ./webpack.config.js",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "watch": "./scripts/watch.js",
    "lint": "eslint --fix --ext .js,.jsx ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "env": {
      "webextensions": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "importSort": {
    ".js, .jsx": {
      "style": "react"
    }
  },
  "devDependencies": {
    "css-loader": "^3.5.3",
    "eslint-config-prettier": "^6.11.0",
    "import-sort-style-react": "^5.1.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "prettier": "^2.0.5",
    "prettier-plugin-import-sort": "^0.0.4",
    "style-loader": "^1.2.1",
    "webpack-cli": "^3.3.11",
    "webpack-extension-reloader": "^1.1.4"
  }
}
