{
  "name": "travo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "1.x",
    "@material-ui/core": "^4.11.0",
    "@material-ui/pickers": "^3.2.10",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^12.1.1",
    "@types/jest": "^26.0.12",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.11.5",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.3.0",
    "date-fns": "^2.15.0",
    "fontsource-roboto": "^2.1.4",
    "lodash": "^4.17.20",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.7.0",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.1",
    "typescript": "^3.9.7",
    "uuid": "^8.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn build:app && yarn build:extension",
    "build:app": "INLINE_RUNTIME_CHUNK=false GENERATE_SOURCEMAP=false react-scripts build",
    "build:extension": "webpack --mode production --config ./webpack.config.js",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "watch": "./scripts/watch.js",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier",
      "airbnb-typescript-prettier"
    ],
    "env": {
      "webextensions": true
    },
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-explicit-any": "off"
    },
    "ignorePatterns": [
      "webpack.config.js",
      "serviceWorker.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "react"
    }
  },
  "devDependencies": {
    "css-loader": "^4.2.2",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "eslint-config-prettier": "^6.11.0",
    "import-sort-style-react": "^5.1.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "prettier": "^2.0.5",
    "prettier-plugin-import-sort": "^0.0.4",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "webpack-cli": "^3.3.12",
    "webpack-extension-reloader": "^1.1.4"
  }
}
