{
  "name": "Travo",
  "version": "1.0.0",
  "manifest_version": 2,
  "browser_specific_settings": {
    "gecko": {
      "id": "travo@hubesco.com"
    }
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["notification/index.js"],
      "css": ["notification/inde.css"]
    }
  ],
  "permissions": ["activeTab", "storage", "tabs"]
}
